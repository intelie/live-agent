{
  "DEBUG": false,
  "temp_dir": "./temp",
  "processes": {
    "chatbot": {
      "type": "chatterbot",
      "enabled": true,
      "alias": "Oliver",
      "live": {
        "host": "http://localhost:8080",
        "username": "admin",
        "password": "admin"
      },
      "monitors": {
        "ReplayRig0": {
          "pretest": {
            "type": "pretest_monitor",
            "enabled": true,
            "monitor": {
              "mnemonics": {
                "index": "ETIM",
                "probe1 depth": "DVP1",
                "probe1 pressure": "BRD1",
                "probe1 pretest volume": "PTV1",
                "probe2 depth": "DVP2",
                "probe2 pressure": "BRD2",
                "probe2 pretest volume": "PTV2"
              },
              "index_mnemonic": "ETIM",
              "window_duration": 60,
              "buildup_duration": 30,
              "buildup_wait_period": 120,
              "probes": {
                "1": {
                  "depth_mnemonic": "DVP1",
                  "pressure_mnemonic": "BRD1",
                  "pretest_volume_mnemonic": "PTV1"
                },
                "2": {
                  "depth_mnemonic": "DVP2",
                  "pressure_mnemonic": "BRD2",
                  "pretest_volume_mnemonic": "PTV2"
                }
              }
            },
            "destination": {
              "author": {"id": 2, "name": "🤖  Pretest monitor"},
              "dashboard": {"id": 17, "name": "Well 0: Pretest"},
              "message_event": {"event_type": "raw_well0", "mnemonic": "MSG"}
            }
          },
          "flowrate": {
            "type": "flowrate_monitor",
            "enabled": true,
            "monitor": {
              "mnemonics": {
                "current value": "current_value",
                "number of changes": "num_changes",
                "values": "values_list",
                "initial timestamp": "start",
                "final timestamp": "end"
              },
              "index_mnemonic": "timestamp",
              "flowrate_mnemonics": ["POTFR", "POFR", "POFR2"],
              "max_threshold": 5,
              "window_duration": 60,
              "sampling_frequency": 5,
              "precision": 1
            },
            "destination": {
              "author": {"id": 2, "name": "🤖  Flowrate monitor"}
            }
          }
        },
        "ReplayRig3": {
          "pretest": {
            "type": "pretest_monitor",
            "enabled": true,
            "monitor": {
              "mnemonics": {
                "index": "ETIM",
                "probe1 depth": "PQDVP1",
                "probe1 pressure": "QDPRESS_PQ1",
                "probe1 pretest volume": "PQPTV1",
                "probe2 depth": "PQDVP2",
                "probe2 pressure": "QDPRESS_PQ2",
                "probe2 pretest volume": "PQPTV2"
              },
              "index_mnemonic": "ETIM",
              "window_duration": 60,
              "buildup_duration": 30,
              "buildup_wait_period": 120,
              "probes": {
                "1": {
                  "depth_mnemonic": "PQDVP1",
                  "pressure_mnemonic": "QDPRESS_PQ1",
                  "pretest_volume_mnemonic": "PQPTV1"
                },
                "2": {
                  "depth_mnemonic": "PQDVP2",
                  "pressure_mnemonic": "QDPRESS_PQ2",
                  "pretest_volume_mnemonic": "PQPTV2"
                }
              }
            },
            "destination": {
              "author": {"id": 2, "name": "🤖  Pretest monitor"},
              "dashboard": {"id": 18, "name": "Well 3: Pretest"},
              "message_event": {"event_type": "raw_well3", "mnemonic": "MSG"}
            }
          },
          "sampling": {
            "type": "sampling_monitor",
            "enabled": true,
            "monitor": {
              "mnemonics": {
                "index": "ETIM",
                "probe1 depth": "PQDVP1",
                "probe1 pressure": "QDPRESS_PQ1",
                "probe1 pumpout flowrate": "POFR",
                "probe1 pump motor speed": "POMS",
                "probe2 depth": "PQDVP2",
                "probe2 pressure": "QDPRESS_PQ2",
                "probe2 pumpout flowrate": "POFR2",
                "probe2 pump motor speed": "POMS2"
              },
              "index_mnemonic": "ETIM",
              "window_duration": 60,
              "buildup_duration": 30,
              "buildup_wait_period": 120,
              "focused_flow_grace_period": 180,
              "probes": {
                "1": {
                  "depth_mnemonic": "PQDVP1",
                  "pressure_mnemonic": "QDPRESS_PQ1",
                  "pumpout_flowrate_mnemonic": "POFR",
                  "pump_motor_speed_mnemonic": "POMS"
                },
                "2": {
                  "depth_mnemonic": "PQDVP2",
                  "pressure_mnemonic": "QDPRESS_PQ2",
                  "pumpout_flowrate_mnemonic": "POFR2",
                  "pump_motor_speed_mnemonic": "POMS2"
                }
              }
            },
            "destination": {
              "author": {"id": 2, "name": "🤖  Sampling monitor"},
              "dashboard": {"id": 21, "name": "Well 3: Sampling"},
              "message_event": {"event_type": "raw_well3", "mnemonic": "MSG"}
            }
          },
          "flowrate": {
            "type": "flowrate_monitor",
            "enabled": true,
            "monitor": {
              "mnemonics": {
                "current value": "current_value",
                "number of changes": "num_changes",
                "values": "values_list",
                "initial timestamp": "start",
                "final timestamp": "end"
              },
              "index_mnemonic": "timestamp",
              "flowrate_mnemonics": ["POTFR", "POFR", "POFR2"],
              "max_threshold": 5,
              "window_duration": 60,
              "sampling_frequency": 5,
              "precision": 1
            },
            "destination": {
              "author": {"id": 2, "name": "🤖  Flowrate monitor"}
            }
          }
        }
      },
      "destination": {
        "name": "rest-agent",
        "author": {"id": 2, "name": "Oliver"}
      }
    }
  },
  "output": {
    "tcp": {
      "type": "tcp",
      "ip": "127.0.0.1",
      "port": 17042
    },
    "rest-agent": {
      "type": "rest",
      "url": "http://localhost:8080/services/plugin-restinput/shell/",
      "username": "live-agent",
      "password": "live-agent"
    },
    "rest-log": {
      "type": "rest",
      "url": "http://localhost:8080/services/plugin-restinput/shell/",
      "username": "live-agent",
      "password": "live-agent",
      "level": "info",
      "event_type": "dda_log"
    }
  }
}
