{
  "DEBUG": false,
  "temp_dir": "./temp",
  "processes": {
    "well0_replay": {
      "type": "las_file",
      "path_list" : [
        "./input_files/well0-MDT_OFA_CMR_103LTP.las",
        "./input_files/well0-MDT_OFA_CMR_104LTP.las",
        "./input_files/well0-MDT_OFA_CMR_106LTP.las"
      ],
      "index_mnemonic": "TIME",
      "destination": {
        "name": "events",
        "event_type": "raw_well0"
      },
      "enabled": true
    },
    "well3_replay": {
      "type": "las_file",
      "path_list" : [
        "./input_files/well3-MDT_OFA_029LTP_Sample1.txt",
        "./input_files/well3-MDT_OFA_030LTP_Sample2.txt"
      ],
      "index_mnemonic": "TIME",
      "destination": {
        "name": "rest_events",
        "event_type": "raw_well3"
      },
      "enabled": true
    },
    "well0_flowrate": {
      "type": "flowrate_monitor",
      "request": {
        "url": "http://localhost:8080/open/plugin-liverig/liverig-simplerest/external_agent/query/well0?username=teste&password=teste",
        "interval": 60
      },
      "monitor": {
        "index_mnemonic": "ETIM",
        "flowrate_mnemonics": ["POTFR"],
        "max_threshold": 10,
        "window_duration": 600
      },
      "destination": {
        "name": "messenger",
        "author": {"id": 1, "name": "Flowrate monitor"},
        "room": {"id": "eia02b6rencfqrjfq5um06842o"}
      },
      "enabled": false
    },
    "well3_flowrate": {
      "type": "flowrate_monitor",
      "request": {
        "url": "http://localhost:8080/open/plugin-liverig/liverig-simplerest/external_agent/query/well3?username=teste&password=teste",
        "interval": 60
      },
      "monitor": {
        "index_mnemonic": "ETIM",
        "flowrate_mnemonics": ["POTFR"],
        "max_threshold": 10,
        "window_duration": 600
      },
      "destination": {
        "name": "rest_messenger",
        "author": {"id": 1, "name": "Flowrate monitor"},
        "room": {"id": "f1v7nljg8on2v2h5tm6v9bmdug"}
      },
      "enabled": false
    },
    "well0_pretest": {
      "type": "pretest_monitor",
      "enabled": true,
      "request": {
        "url": "http://localhost:8080/open/plugin-liverig/liverig-simplerest/pretest_monitor/query/well0?username=teste&password=teste",
        "interval": 15
      },
      "monitor": {
        "index_mnemonic": "ETIM",
        "window_duration": 60,
        "buildup_duration": 30,
        "buildup_wait_period": 120,
        "probes": {
          "1": {
            "depth_mnemonic": "DVP1",
            "pressure_mnemonic": "BRD1",
            "pretest_volume_mnemonic": "PTV1",
            "pumpout_flowrate_mnemonic": "POTFR",
            "pumpout_volume_mnemonic": "POTCV"
          },
          "2": {
            "depth_mnemonic": "DVP2",
            "pressure_mnemonic": "BRD2",
            "pretest_volume_mnemonic": "PTV2",
            "pumpout_flowrate_mnemonic": "POTFR",
            "pumpout_volume_mnemonic": "POTCV"
          }
        }
      },
      "destination": {
        "name": "messenger",
        "author": {"id": 1, "name": "Pretest monitor"},
        "room": {"id": "eia02b6rencfqrjfq5um06842o"}
      }
    },
    "well3_pretest": {
      "type": "pretest_monitor",
      "enabled": true,
      "request": {
        "url": "http://localhost:8080/open/plugin-liverig/liverig-simplerest/pretest_monitor/query/well3?username=teste&password=teste",
        "interval": 15
      },
      "monitor": {
        "index_mnemonic": "ETIM",
        "window_duration": 60,
        "buildup_duration": 30,
        "buildup_wait_period": 120,
        "probes": {
          "1": {
            "depth_mnemonic": "PQDVP1",
            "pressure_mnemonic": "QDPRESS_PQ1",
            "pretest_volume_mnemonic": "PQPTV1",
            "pumpout_flowrate_mnemonic": "POTFR",
            "pumpout_volume_mnemonic": "POTCV"
          },
          "2": {
            "depth_mnemonic": "PQDVP2",
            "pressure_mnemonic": "QDPRESS_PQ2",
            "pretest_volume_mnemonic": "PQPTV2",
            "pumpout_flowrate_mnemonic": "POTFR",
            "pumpout_volume_mnemonic": "POTCV"
          }
        }
      },
      "destination": {
        "name": "rest_messenger",
        "author": {"id": 1, "name": "Pretest monitor"},
        "room": {"id": "f1v7nljg8on2v2h5tm6v9bmdug"}
      }
    }
  },
  "output": {
    "events": {
      "type": "raw_over_tcp",
      "ip": "127.0.0.1",
      "port": 17042
    },
    "rest_events": {
      "type": "raw_over_rest",
      "url": "http://localhost:8080/services/plugin-restinput/shell/",
      "username": "admin",
      "password": "admin"
    },
    "messenger": {
      "type": "chat_over_tcp",
      "ip": "127.0.0.1",
      "port": 17042
    },
    "rest_messenger": {
      "type": "chat_over_rest",
      "url": "http://localhost:8080/services/plugin-restinput/shell/",
      "username": "admin",
      "password": "admin"
    }
  }
}
