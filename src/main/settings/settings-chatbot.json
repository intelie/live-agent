{
  "DEBUG": false,
  "temp_dir": "./temp",
  "enabled_modules": [
    "chatbot",
    "live",
    "mdt"
  ],
  "processes": {
    "chatbot": {
      "type": "chatterbot",
      "enabled": true,
      "alias": "Oliver",
      "logic_adapters": [
        {
            "import_path": "chatterbot.logic.BestMatch",
            "default_response": "I am sorry, but I do not understand.",
            "maximum_similarity_threshold": 0.90
        },
        "chatbot.logic_adapters.internal.StateDebugAdapter",
        "chatbot.logic_adapters.internal.AdapterReloaderAdapter",
        "chatbot.logic_adapters.internal.BotFeaturesAdapter",
        "live.logic_adapters.live_asset.AssetListAdapter",
        "live.logic_adapters.live_asset.AssetSelectionAdapter",
        "live.logic_adapters.live_functions.AutoAnalysisAdapter",
        "live.logic_adapters.pipes_query.EtimQueryAdapter",
        "live.logic_adapters.pipes_query.CurrentValueQueryAdapter",
        "chatbot.logic_adapters.monitors.MonitorControlAdapter",
        "torque_and_drag.logic_adapters.torque_and_drag.TorqueAndDragAdapter",
        "chatbot.logic_adapters.variables.BotVariablesAdapter"
      ],
      "live": {
        "host": "http://localhost:8080",
        "username": "admin",
        "password": "admin"
      },
      "monitors": {
        "ReplayRig0": {
          "pretest": {
            "type": "pretest_monitor",
            "enabled": true,
            "monitor": {
              "window_duration": 60,
              "buildup_duration": 30,
              "buildup_wait_period": 120,
              "mnemonics": {
                "index": "ETIM",
                "pretest number": "PTNO",
                "probe1 depth mnemonic": "DVP1",
                "probe1 pressure mnemonic": "BRD1",
                "probe1 pretest volume mnemonic": "PTV1",
                "probe1 temperature mnemonic": "B1TR",
                "probe2 depth mnemonic": "DVP2",
                "probe2 pressure mnemonic": "BRD2",
                "probe2 pretest volume mnemonic": "PTV2",
                "probe2 temperature mnemonic": "B2TR"
              },
              "probes": ["1", "2"]
            },
            "destination": {
              "author": {"id": 2, "name": "  Pretest monitor"},
              "dashboard": {"id": 17, "name": "Well 0: Pretest"},
              "message_event": {"event_type": "raw_well0", "mnemonic": "MSG"}
            }
          },
          "flowrate": {
            "type": "flowrate_monitor",
            "enabled": true,
            "monitor": {
              "mnemonics": {
                "index": "timestamp",
                "current value": "current_value",
                "number of changes": "num_changes",
                "values": "values_list",
                "initial timestamp": "start",
                "final timestamp": "end"
              },
              "flowrate_mnemonics": ["POTFR", "POFR", "POFR2"],
              "max_threshold": 5,
              "window_duration": 60,
              "sampling_frequency": 5,
              "precision": 0
            },
            "destination": {
              "author": {"id": 2, "name": "  Flowrate monitor"}
            }
          }
        },
        "ReplayRig3": {
          "pretest": {
            "type": "pretest_monitor",
            "enabled": true,
            "monitor": {
              "window_duration": 60,
              "buildup_duration": 30,
              "buildup_wait_period": 120,
              "mnemonics": {
                "index": "ETIM",
                "pretest number": "PTNO",
                "probe1 depth mnemonic": "PQDVP1",
                "probe1 pressure mnemonic": "QDPRESS_PQ1",
                "probe1 pretest volume mnemonic": "PQPTV1",
                "probe1 temperature mnemonic": "PQ1TR",
                "probe2 depth mnemonic": "PQDVP2",
                "probe2 pressure mnemonic": "QDPRESS_PQ2",
                "probe2 pretest volume mnemonic": "PQPTV2",
                "probe2 temperature mnemonic": "PQ2TR"
              },
              "probes": ["1", "2"]
            },
            "destination": {
              "author": {"id": 2, "name": "  Pretest monitor"},
              "dashboard": {"id": 18, "name": "Well 3: Pretest"},
              "message_event": {"event_type": "raw_well3", "mnemonic": "MSG"}
            }
          },
          "sampling": {
            "type": "sampling_monitor",
            "enabled": true,
            "monitor": {
              "mnemonics": {
                "index": "ETIM",
                "probe1 depth mnemonic": "PQDVP1",
                "probe1 pressure mnemonic": "QDPRESS_PQ1",
                "probe1 pumpout flowrate mnemonic": "POFR",
                "probe1 pump motor speed mnemonic": "POMS",
                "probe2 depth mnemonic": "PQDVP2",
                "probe2 pressure mnemonic": "QDPRESS_PQ2",
                "probe2 pumpout flowrate mnemonic": "POFR2",
                "probe2 pump motor speed mnemonic": "POMS2"
              },
              "window_duration": 60,
              "buildup_duration": 30,
              "buildup_wait_period": 120,
              "focused_flow_grace_period": 180,
              "probes": ["1", "2"]
            },
            "destination": {
              "author": {"id": 2, "name": "  Sampling monitor"},
              "dashboard": {"id": 21, "name": "Well 3: Sampling"},
              "message_event": {"event_type": "raw_well3", "mnemonic": "MSG"}
            }
          },
          "flowrate": {
            "type": "flowrate_monitor",
            "enabled": true,
            "monitor": {
              "mnemonics": {
                "index": "timestamp",
                "current value": "current_value",
                "number of changes": "num_changes",
                "values": "values_list",
                "initial timestamp": "start",
                "final timestamp": "end"
              },
              "flowrate_mnemonics": ["POTFR", "POFR", "POFR2"],
              "max_threshold": 5,
              "window_duration": 60,
              "sampling_frequency": 5,
              "precision": 0
            },
            "destination": {
              "author": {"id": 2, "name": "  Flowrate monitor"}
            }
          },
          "flowrate_linearity": {
            "type": "flowrate_linearity_monitor",
            "enabled": true,
            "monitor": {
              "mnemonics": {
                "ratio": "ratio",
                "mean ratio": "mratio"
              },
              "index_mnemonic": "timestamp",
              "window_duration": 5,
              "sampling_interval": 1,
              "threshold": 0.1,

              "flowrate_mnemonic": "POFR",
              "pressure_mnemonic": "QDPRESS_PQ1"
            },
            "destination": {
              "author": {"id": 2, "name": "  Flowrate Linearity monitor"}
            }
          }
        },
        "Zatko": {
          "alert_reference": {
            "type": "alert_reference_monitor",
            "enabled": true,
            "monitor": {
              "span": null
            },
            "destination": {
              "author": {"id": 2, "name": "  Alert Reference Monitor"}
            }
          }
        }
      },
      "destination": {
        "name": "rest-agent",
        "author": {"id": 2, "name": "Oliver"}
      }
    }
  },
  "output": {
    "tcp": {
      "type": "tcp",
      "ip": "127.0.0.1",
      "port": 17042
    },
    "rest-agent": {
      "type": "rest",
      "url": "http://localhost:8080/services/plugin-restinput/shell/",
      "username": "live-agent",
      "password": "live-agent"
    },
    "rest-log": {
      "type": "rest",
      "url": "http://localhost:8080/services/plugin-restinput/shell/",
      "username": "live-agent",
      "password": "live-agent",
      "level": "info",
      "event_type": "dda_log"
    }
  }
}
